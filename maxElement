public class maxElement {
    public static void main(String[] args) {

        int[] arr= {1,32,56,76,22,88};
        System.out.println(Arrays.toString(arr));
        System.out.println(max(arr));
    }
   
    static int max(int[] arr){
        int maxval = arr[0];
        for (int i = 0; i < arr.length; i++) {
            if (arr[i]>maxval){
                maxval=arr[i];
            }
        }
        return maxval;
    }
}
